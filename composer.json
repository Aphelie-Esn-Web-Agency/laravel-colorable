{
  "name": "aphelie/colorable",
  "description": "A Laravel package to add colorable trait to models",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=7.3",
    "illuminate/support": ">5.0"
  },
  "autoload": {
    "psr-4": {
      "Aphelie\\Colorable\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Aphelie\\Colorable\\ColorableServiceProvider"
      ],
      "aliases": {
        "Acoss": "Aphelie\\Colorable\\Facade\\Acoss"
      }
    }
  }
}